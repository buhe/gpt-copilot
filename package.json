{
    "name": "gpt-copilot",
    "version": "0.0.1",
    "private": true,
    "engines": {
        "vscode": "^1.74.0"
    },
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "gpt-copilot.test",
                "title": "GPT Copilot"
            },
            {
                "command": "gpt-copilot.explain_selection",
                "title": "GPT - Explain Selection"
            },
            {
                "command": "gpt-copilot.ask_about_selection",
                "title": "GPT - Ask About Selection"
            },
            {
                "command": "gpt-copilot.explain_file",
                "title": "GPT - Explain File"
            },
            {
                "command": "gpt-copilot.ask_about_file",
                "title": "GPT - Ask About File"
            },
            {
                "command": "gpt-copilot.ask_anything",
                "title": "GPT - Ask Anything"
            },
            {
                "command": "gpt-copilot.setup",
                "title": "GPT - Setup"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "editorHasSelection",
                    "command": "gpt-copilot.explain_selection",
                    "group": "z_commands"
                },
                {
                    "when": "editorHasSelection",
                    "command": "gpt-copilot.ask_about_selection",
                    "group": "z_commands"
                },
                {
                    "when": "!editorHasSelection",
                    "command": "gpt-copilot.explain_file",
                    "group": "z_commands"
                },
                {
                    "when": "!editorHasSelection",
                    "command": "gpt-copilot.ask_about_file",
                    "group": "z_commands"
                },
                {
                    "command": "gpt-copilot.ask_anything",
                    "group": "z_commands"
                },
                {
                    "command": "gpt-copilot.test",
                    "group": "z_commands"
                }
                
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint \"src/**/*.ts\"",
        "watch": "tsc -watch -p ./"
    },
    "dependencies": {
        "@vscode/webview-ui-toolkit": "1.2.0"
    },
    "devDependencies": {
        "@types/node": "^16.11.7",
        "@types/vscode": "^1.73.0",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "eslint": "^8.26.0",
        "typescript": "^4.8.4"
    }
  }